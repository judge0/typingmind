{
    "name": "code_execution",
    "description": "Run code in various programming languages",
    "parameters": {
        "type": "object",
        "properties": {
            "source_code": {
                "type": "string",
                "description": "A source code snippet"
            },
            "language": {
                "type": "string",
                "enum": ["c", "cpp", "go", "java", "javascript", "python", "ruby"],
                "description": "A name of the programming language"
            }
        },
        "required": [
            "source_code",
            "language"
        ]
    }
}
